{
  "name": "react-text-adventure",
  "version": "1.0.0",
  "description": "Create text adventures using redux, react, and entity component system",
  "author": "agray5",
  "license": "MIT",
  "repository": "text-adventure",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start:example": "cd ./example && npm start",
